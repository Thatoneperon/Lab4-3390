<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="Views.StudentGUI">
  <grid id="27dc6" binding="panel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <scrollpane id="c8701">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="line" title="Student List"/>
        <children>
          <component id="b80a6" class="javax.swing.JList" binding="studentList">
            <constraints/>
            <properties/>
          </component>
        </children>
      </scrollpane>
      <grid id="5ff70" binding="studentInput" layout-manager="GridBagLayout">
        <constraints border-constraint="East"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="2ff91" class="javax.swing.JLabel" binding="labelFirstName">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag top="2" left="2" bottom="2" right="2" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <text value="First Name"/>
            </properties>
          </component>
          <component id="c21f7" class="javax.swing.JTextField" binding="inputFirstName">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
              <gridbag top="2" left="2" bottom="2" right="2" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <columns value="15"/>
            </properties>
          </component>
          <component id="7860b" class="javax.swing.JLabel" binding="labelLastName">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag top="2" left="2" bottom="2" right="2" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <text value="Last Name"/>
            </properties>
          </component>
          <component id="e6e33" class="javax.swing.JTextField" binding="inputLastName">
            <constraints>
              <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
              <gridbag top="2" left="2" bottom="2" right="2" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <columns value="15"/>
            </properties>
          </component>
          <component id="31a40" class="javax.swing.JLabel" binding="labelStudentId">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag top="2" left="2" bottom="2" right="2" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <text value="Student ID"/>
            </properties>
          </component>
          <component id="a9267" class="javax.swing.JTextField" binding="inputStudentId">
            <constraints>
              <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
              <gridbag top="2" left="2" bottom="2" right="2" weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <columns value="15"/>
            </properties>
          </component>
          <vspacer id="a1c05">
            <constraints>
              <grid row="3" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="1.0"/>
            </constraints>
          </vspacer>
        </children>
      </grid>
      <grid id="ad28" binding="studentActions" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="1cd3f" class="javax.swing.JButton" binding="buttonAddStudent">
            <constraints/>
            <properties>
              <text value="Add Student"/>
            </properties>
          </component>
          <component id="52ca" class="javax.swing.JButton" binding="buttonRemoveStudent">
            <constraints/>
            <properties>
              <text value="Remove Student"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
